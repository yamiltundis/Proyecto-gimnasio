
generator client {
  provider   = "prisma-client-js"
}

datasource db {
  provider = "mysql"
}

model Cliente {
  id     Int    @id @default(autoincrement())
  nombre String
  apellido String
  dni Int
  fechaNacimiento DateTime
  email  String @unique
  foto String
  rol String
}

model TipoClase {
  id Int @id @default(autoincrement())
  nombre String
}